## This is the main secureserver config file
## Properties should be set in the same manner as bash variables
## Example:
## property=value

# This is the secureserver api endpoint
service_endpoint="http://146.185.148.96:4567/api/post"

# Frequency in seconds which tells the agent how often to report back to the service
report_frequency=60

# Your api key
api_key=REPLACE_WITH_YOUR_API_KEY

# Connection timeout in seconds
connect_timeout=10

# Max connection time in seconds
max_time=10

# Loglevel (INFO or DEBUG)
loglevel=INFO
